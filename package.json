{
  "name": "lifteller",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "compile": "tsc --noEmit -p . --pretty",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "pnpm exec prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:fix": "pnpm exec prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "ios:pod-install": "cd ios && pod install && cd ..",
    "ios:build": "cd ios && xcodebuild -workspace lifteller.xcworkspace -scheme lifteller -configuration Debug -sdk iphonesimulator -derivedDataPath build",
    "ios:build-device": "cd ios && xcodebuild -workspace lifteller.xcworkspace -scheme lifteller -configuration Debug -sdk iphoneos -derivedDataPath build CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO",
    "android:build": "cd android && ./gradlew assembleDebug",
    "android:build-device": "cd android && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.8",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.25",
    "expo-av": "^16.0.8",
    "expo-blur": "^15.0.8",
    "expo-camera": "~16.0.8",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "~6.0.18",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "^17.0.9",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "~8.0.9",
    "expo-router": "~6.0.15",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "lucide-react-native": "^0.555.0",
    "moti": "^0.30.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-international-phone-number": "^0.11.4",
    "react-native-pager-view": "^7.0.2",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.15.1",
    "react-native-tab-view": "^4.2.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "^0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.17",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "react-native-svg-transformer": "^1.5.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
